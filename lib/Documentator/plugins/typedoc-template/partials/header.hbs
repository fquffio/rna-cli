<header class="{{#unless model.parent}}home{{/unless}}">
    <div class="container container-row">
        {{#if model.parent}}
            <a href="{{relativeURL "index.html"}}" class="title">{{project.name}}</a>
        {{else}}
            <h1 class="title">{{project.name}}</h1>
        {{/if}}
        <div id="tsd-search" data-index="{{relativeURL "assets/js/search.js"}}" data-base="{{relativeURL "./"}}">
            <div class="field">
                <label for="tsd-search-field" class="tsd-widget search no-caption">Search</label>
                <input id="tsd-search-field" type="text" />
            </div>

            <ul class="results">
                <li class="state loading">Preparing search index...</li>
                <li class="state failure">The search index is not available</li>
            </ul>
        </div>
    </div>
    {{#if model.parent}}
    <div class="tsd-page-title">
        <div class="container">
            {{#if model.parent.parent}}
            <ul class="tsd-breadcumb">
                {{#with model.parent}}{{> breadcrumb}}{{/with}}
            </ul>
            {{/if}}
            <h1>{{#compact}}
                {{model.kindString}}&nbsp;
                {{model.name}}
                {{#if model.typeParameters}}
                    &lt;
                    {{#each model.typeParameters}}
                        {{#if @index}},&nbsp;{{/if}}
                        {{name}}
                    {{/each}}
                    &gt;
                {{/if}}
            {{/compact}}</h1>
        </div>
    </div>
    {{/if}}
</header>
